{
  "name": "react-next-starter",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "plop": "plop",
    "test": "run-p test:*",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:unit:update": "jest --updateSnapshot",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "lint": "run-p lint:*",
    "lint:types": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:format": "yarn prettier --check --ignore-unknown .",
    "storybook": "start-storybook -c src/storybook/config -p 6006",
    "build-storybook": "build-storybook -c src/storybook/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@emotion/react": "11.1.4",
    "@emotion/styled": "11.0.0",
    "@fontsource/roboto": "4.1.0",
    "@material-ui/core": "5.0.0-alpha.23",
    "@material-ui/icons": "4.11.2",
    "@reduxjs/toolkit": "1.5.0",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux-devtools": "3.7.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.1.11",
    "@storybook/addon-essentials": "6.1.11",
    "@storybook/addon-links": "6.1.11",
    "@storybook/react": "6.1.11",
    "@testing-library/dom": "7.29.4",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.3",
    "@testing-library/react-hooks": "3.7.0",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.21",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.14",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "cypress": "6.2.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "plop": "2.7.4",
    "prettier": "2.2.1",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "typescript": "4.1.3"
  }
}
